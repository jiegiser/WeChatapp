/** wxss **/
/** 修改默认的navigator点击态 **/
page{
    background: #ebebeb;
}
.navigator-hover {
    color:blue;
}
/** 自定义其他点击态样式类 **/
.other-navigator-hover {
    color:red;
}
.navigator{
    width: 100%;
}
.nav{
    width:100%;
    height: 40px;
    background: #fff;
    line-height: 40px;
    font-size: 13px;
    text-indent: 10px;
    position: relative;
}
.nav text{
    margin-left:30px;
}
.nav_img{
    position:absolute;
    top:9px;
    width:20px;
    height:20px;
}

.nav_top{
    margin-top:18px;
}

.nav_border{
    border-top:1px solid #ddd;
}
